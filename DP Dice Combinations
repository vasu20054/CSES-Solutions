#include<bits/stdc++.h>
using namespace std;
#define int long long 
#define pb push_back
#define dst(v) sort(v.begin(),v.end(),greater<long long>())
#define ast(v) sort(v.begin(),v.end());
#define vi vector<int>
#define vvi vector<vector<int>>
#define pi pair<int,int> 
#define vvpi vector<vector<pair<int,int>>>
#define input(vec,n) for(int i=0;i<n;i++) cin>>vec[i];
void print(vi a){
    for(auto it:a) cout<<(it)<<" ";
    cout<<endl;
}      
 
/*CODE*/
int mod=1e9+7;
void Solve(){
        int n; cin>>n;
    vector<int> v(n+1,0);
    v[0]=1;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=6;j++){
            if(j<=i){
                v[i]=(v[i]+v[i-j])%c;
            }
        }
    }
    cout<<v[n]<<endl;
}
 
signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;t=1;
    while(t--) Solve();
}
