#include<bits/stdc++.h>
using namespace std;
#define int long long 
#define pb push_back
#define dst(v) sort(v.begin(),v.end(),greater<long long>())
#define ast(v) sort(v.begin(),v.end());
#define vi vector<int>
#define vvi vector<vector<int>>
#define pi pair<int,int> 
#define vvpi vector<vector<pair<int,int>>>
#define input(vec,n) for(int i=0;i<n;i++) cin>>vec[i];
void print(vi a){
    for(auto it:a) cout<<(it)<<" ";
    cout<<endl;
}      
 
/*CODE*/
int mod=1e9+7;
void Solve(){
    int n,x; cin>>n>>x;
    vector<int> dp(1e6+1,INT_MAX);
    vector<int> v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
        dp[v[i]]=1;
    }
    for(int i=1;i<=x;i++){
        for(int j=0;j<n;j++){
            if(v[j]<=i){
                dp[i]=min(dp[i],dp[i-v[j]]+1);
            }
        }
    }
    if(dp[x]==INT_MAX) cout<<-1<<endl;
    else cout<<dp[x]<<endl;
}
 
signed main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;t=1;
    while(t--) Solve();
}
